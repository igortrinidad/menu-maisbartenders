<?xml version='1.0' encoding='utf-8'?>
<widget id="com.br.menu.maisbartenders" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MaisBartenders</name>
    <description>
        Menu Interativo Mais bartenders.
    </description>
    <author email="contato@maisbartenders.com.br" href="http://maisbartenders.com.br">
        Criado por We-Planner Soluções Tecnológicas
    </author>
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://menu.maisbartenders.com.br" />
    <allow-navigation href="http://menu.maisbartenders.com.br/*" />
    <allow-navigation href="https://maisbartenders.com.br/*" />
    <allow-navigation href="https://www.facebook.com/*" />
    <allow-navigation href="https://fonts.googleapis.com/*" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="3000" />
</widget>
